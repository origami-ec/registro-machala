	<queryString>
		<![CDATA[select string_agg('* ' || rei.nombre, CHR(10) order by rei.nombre) as nombres,
m.id as movimiento, sec.repertorio, sec.fecha, m.num_repertorio,
m.fecha_repertorio, m.num_inscripcion, m.fecha_inscripcion,
m.folio_inicio, m.folio_fin, ac.nombre as acto, li.nombre as libro,
'* ' || ht.nombre_propietario as solicitante, ej.nombre as notaria,
dom.nombre as domicilio, m.secuencia_repertorio, m.secuencia_inscripcion
from secuencias.secuencia_repertorio_mercantil sec
left join app.reg_movimiento m on
(sec.repertorio = m.num_repertorio and sec.anio::varchar = to_char(m.fecha_repertorio, 'yyyy'))
left join app.reg_movimiento_cliente as mc on (mc.movimiento = m.id)
left join app.reg_ente_interviniente as rei on (rei.id = mc.ente_interv)
left join app.reg_acto ac on (ac.id = m.acto)
left join app.reg_libro li on (li.id = m.libro)
left join app.reg_ente_judiciales ej on ej.id = m.ente_judicial
left join app.reg_domicilio dom on dom.id = m.domicilio
left join flow.regp_tareas_tramite tt on (tt.id = m.num_tramite)
left join flow.historico_tramites ht on (ht.id = tt.tramite)
where li.propiedad is false and to_char(sec.fecha, 'dd/MM/yyyy') = $P{FECHA_REPERTORIO}
group by 2, 3, 4, 11, 12, 13, 14, 15
order by sec.repertorio, m.fecha_inscripcion, m.num_inscripcion]]>
	</queryString>
