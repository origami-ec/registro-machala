	<queryString>
		<![CDATA[select li.id, li.num_tramite_rp, li.fecha_creacion, li.sub_total, li.descuento_valor,
li.total_pagar, li.codigo_comprobante, li.numero_comprobante, us.usuario, li.certificado,  COALESCE(UPPER(li.inf_adicional_prof), ' ') as observaciones,
coalesce(fun.apellidos,'')||' '||coalesce(fun.nombres,'') as funcionario,
li.inscripcion, li.reingreso, sol.ci_ruc, li.fecha_ingreso, li.titulo_credito,
case sol.es_persona when true then
coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'') else sol.razon_social end as solicitante,ins.usuario as certificador, ht.revisor, re.usuario as revisor_titulo, a.solvencia
from flow.regp_liquidacion li
left join app.acl_user us on us.id = li.user_creacion
left join app.cat_ente sol on sol.id = li.solicitante
left join app.acl_user ins on ins.id = li.inscriptor
left join app.cat_ente fun on fun.id = us.ente
left join flow.historico_tramites ht on ht.id = li.tramite
left join app.acl_user re on re.id = ht.revisor
LEFT JOIN (
    SELECT liquidacion, MIN(id) AS id_detalle
    FROM flow.regp_liquidacion_detalles
    GROUP BY liquidacion
) ld_min ON ld_min.liquidacion = li.id
LEFT JOIN flow.regp_liquidacion_detalles ld ON ld.id = ld_min.id_detalle
LEFT JOIN app.reg_acto a ON a.id = ld.acto
where
(CASE WHEN $P{USUARIO} > 0 THEN us.id = $P{USUARIO} ELSE us.id > 0 END) and
li.fecha_creacion between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
order by li.num_tramite_rp]]>
	</queryString>
