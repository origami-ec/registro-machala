	<queryString>
		<![CDATA[SELECT row_number() over(ORDER BY m.num_inscripcion ASC, m.num_repertorio ASC) numero,
h.num_tramite, h.fecha_ingreso, l.nombre libro, a.nombre contrato,
m.num_repertorio, m.num_inscripcion, UPPER(u.usuario) usuario,
TRIM(COALESCE(ue.nombres, '') || ' ' || COALESCE(ue.apellidos, '')) nombre_funcionario
FROM app.reg_movimiento m
INNER JOIN flow.regp_tareas_tramite tt ON tt.id = m.num_tramite
INNER JOIN flow.historico_tramites h on h.id = tt.tramite
LEFT JOIN app.reg_libro l ON l.id = m.libro
LEFT JOIN app.reg_acto a ON a.id = m.acto
INNER JOIN app.acl_user u ON u.id = m.user_creador
INNER JOIN app.cat_ente ue ON ue.id = u.ente
WHERE m.fecha_inscripcion::DATE = CAST($P{FECHA} AS DATE)
ORDER BY m.num_inscripcion ASC, m.num_repertorio ASC]]>
	</queryString>
