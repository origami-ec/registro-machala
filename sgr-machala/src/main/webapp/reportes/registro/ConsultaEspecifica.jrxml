	<parameter name="TOMO" class="java.lang.String"/>
	<queryString>
		<![CDATA[select l.nombre as libroname, mov.num_inscripcion as numins, mov.fecha_inscripcion as fecins,
mov.num_repertorio as numrep, a.nombre as actoname, mov.folio_inicio as folioini
from app.reg_movimiento as mov
left outer join app.reg_libro as l on (l.id=mov.libro)
left outer join app.reg_acto as a on (a.id=mov.acto)
where TRUE
AND (CASE WHEN $P{FOLIO}>0 THEN mov.folio_inicio=$P{FOLIO} ELSE mov.folio_inicio>0 END)
AND (CASE WHEN $P{TOMO} is not null THEN mov.num_tomo=$P{TOMO} ELSE mov.num_tomo is not null END)
AND (CASE WHEN $P{LIBRO}>0 THEN mov.libro=$P{LIBRO} ELSE mov.libro>0 END)
AND (CASE WHEN $P{ACTO}>0 THEN mov.acto=$P{ACTO} ELSE mov.acto>0 END)
AND (CASE WHEN $P{REPERTORIO}>0 THEN mov.num_repertorio=$P{REPERTORIO} ELSE mov.num_repertorio>0 END)
AND (CASE WHEN $P{INSCRIPCION}>0 THEN mov.num_inscripcion=$P{INSCRIPCION} ELSE mov.num_inscripcion>=0 END)
AND mov.fecha_inscripcion BETWEEN $P{DESDE} AND $P{HASTA}
order by mov.fecha_inscripcion asc, l.id asc, mov.num_inscripcion asc]]>
	</queryString>
