	<subDataset name="tareas" uuid="e61ef07c-6d3c-4e60-b3c2-b46052ab1a2b">
		<parameter name="SOLICITUD" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select det.*
from administrativo.solicitud_bienes_det det
inner join administrativo.solicitud_bienes sol on det.solicitud = sol.id
where det.solicitud =  $P{SOLICITUD}]]>
		</queryString>
		<field name="id" class="java.lang.Long"/>
		<field name="solicitud" class="java.lang.Long"/>
		<field name="descripcion" class="java.lang.String"/>
		<field name="cantidad" class="java.lang.Integer"/>
		<field name="unidad" class="java.lang.String"/>
		<field name="caracteristicas" class="java.lang.String"/>
		<field name="observaciones" class="java.lang.String"/>
	</subDataset>
	<parameter name="HEADER_URL" class="java.lang.String"/>
	<parameter name="WATERMARK_URL" class="java.lang.String"/>
	<parameter name="FOOTER_URL" class="java.lang.String"/>
	<parameter name="ID" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select
	ti.valor asunto ,
	mo.valor motivo_permiso,
	sol.fecha_inicio_permiso,
	sol.fecha_fin_permiso,
	sol.cargo_funcionario,
	sol.fecha_ingreso,
	(th_per.nombres || ' ' ||  th_per.apellidos) para,
	(fun_per.nombres || ' ' ||  fun_per.apellidos) de,
	(au_per.nombres || ' ' ||  au_per.apellidos) autorizado,
	sol.detalle, sol.numero_solicitud, sol.fecha_ingreso,
	ti.codename,
	(
	  select
			case
				when rol.acl_rol = 50 then 'COORDINADOR ADMINISTRATIVO Y FINANCIERO (E)'
				when rol.acl_rol = 19 then 'REGISTRADORA DE LA PROPIEDAD DEL CANTÓN LOJA'
				when rol.acl_rol = 18 then 'COORDINADOR DE OPERACIONES REGISTRALES'
			else 'AUTORIZADO' END
		from app.acl_user_has_rol rol
		where rol.acl_user = au.id and rol.acl_rol in (18,19,50) limit 1
	) rol

from administrativo.solicitud_permisos sol

inner join app.ctlg_item ti on sol.tipo = ti.id
inner join app.ctlg_item mo on sol.motivo = mo.id


inner join app.acl_user fun on sol.solicitante = fun.id
inner join app.cat_ente fun_per on fun.ente = fun_per.id

inner join app.acl_user th on sol.talento_humano = th.id
inner join app.cat_ente th_per on th.ente = th_per.id

left outer join app.acl_user au on sol.autorizado = au.id
left outer join app.cat_ente au_per on au.ente = au_per.id

where sol.id =  $P{ID}]]>
	</queryString>
