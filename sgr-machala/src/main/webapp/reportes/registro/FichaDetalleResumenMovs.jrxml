	<queryString>
		<![CDATA[select mov.num_inscripcion, mov.fecha_inscripcion, (mov.num_paginas_contabilizada +
mov.num_pagina_razon + mov.num_pagina_inscripcion) as folio_inicio,
acto.nombre as nombre_acto, mov.num_repertorio, libro.nombre as nombre_libro
from app.reg_movimiento_ficha as mov_ficha
