	<queryString>
		<![CDATA[select nc.fecha_emision, nc.motivo, nc.clave_acceso, nc.numero_documento, nc.numero_autorizacion,
nc.valor_total, nc.estado, nc.num_comprobante_modifica as factura, sol.ci_ruc as documento,
CASE sol.es_persona WHEN TRUE THEN coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'')
ELSE sol.razon_social END AS solicitante, liq.fecha_ingreso, liq.num_tramite_rp,
us.usuario as cajero
from financiero.ren_nota_credito nc
left join flow.regp_liquidacion liq on nc.factura = liq.id
left join financiero.ren_cajero ca on ca.id = nc.caja_emision
left join app.acl_user us on us.id = ca.usuario
left join app.cat_ente sol on sol.id = liq.solicitante
where (CASE WHEN $P{USUARIO}>0 THEN liq.user_ingreso=$P{USUARIO} ELSE liq.user_ingreso>0 END)
and nc.fecha_emision between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
order by nc.fecha_emision]]>
	</queryString>
