	<queryString>
		<![CDATA[SELECT
    nd.tramite, nd.usuario_reingresa, li.num_tramite_rp,
    CASE
        WHEN nd.asunto ILIKE 'negativa%' THEN 'N'
        ELSE 'D'
    END AS tipo,
	coalesce(fun.apellidos,'')||' '||coalesce(fun.nombres,'') as funcionario_ventanilla,
	coalesce(fun_a.apellidos,'')||' '||coalesce(fun_a.nombres,'') as funcionario,
    nd.fecha_reingreso, nd.fecha_ingreso as fecha_revision,
    --nd.usuario_reingresa AS funcionario,
    li.fecha_creacion as fecha_solicitud,li.titulo_credito,
    li.inscripcion, li.sub_total, li.descuento_valor, li.total_pagar
FROM flow.regp_nota_devolutiva nd
LEFT JOIN flow.historico_tramites ht ON ht.id = nd.tramite
LEFT JOIN flow.regp_liquidacion li ON li.num_tramite_rp = ht.num_tramite
left join app.acl_user us on us.usuario = nd.usuario_reingresa
left join app.acl_user ab on ab.usuario = nd.elaborado
left join app.cat_ente fun on fun.id = us.ente
left join app.cat_ente fun_a on fun_a.id = ab.ente
WHERE
    (CASE WHEN $P{USUARIO} > 0 THEN us.id = $P{USUARIO} ELSE us.id > 0 END) and
    nd.fecha_reingreso BETWEEN
        to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')
        AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')
ORDER BY nd.tramite;]]>
	</queryString>
