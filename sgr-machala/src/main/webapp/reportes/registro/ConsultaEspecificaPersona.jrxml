	<queryString>
		<![CDATA[select l.nombre as libroname, mov.num_inscripcion as numins,
mov.fecha_inscripcion as fecins, mov.num_repertorio as numrep,
a.nombre as actoname, mov.folio_inicio as folioini, pa.papel,
mov.num_tomo
from app.reg_ente_interviniente renti
left join app.reg_movimiento_cliente as mc on (mc.ente_interv=renti.id)
left join app.reg_movimiento as mov on (mov.id=mc.movimiento)
left join app.reg_libro as l on (l.id=mov.libro)
left join app.reg_acto as a on (a.id=mov.acto)
left join app.reg_papel as pa on (pa.id=mc.papel)
where renti.ced_ruc=$P{ENTE} order by mov.fecha_inscripcion]]>
	</queryString>
