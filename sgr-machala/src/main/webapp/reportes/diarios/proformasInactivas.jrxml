	<queryString>
		<![CDATA[select
liq.num_tramite_rp,
liq.fecha_creacion,
initcap(coalesce(ent.nombres,'')||' '||coalesce(ent.apellidos,'')) benecifiario,
us.usuario,
liq.fecha_ingreso,
liq.sub_total,
liq.total_pagar,
act.nombre acto,
dl.valor_unitario,
dl.descuento,
dl.valor_total,
dl.cantidad,
CASE WHEN liq.inf_adicional = '' OR liq.inf_adicional IS NULL THEN ' ' ELSE liq.inf_adicional END AS inf_adicional
from flow.regp_liquidacion liq
left join flow.regp_liquidacion_detalles dl on dl.liquidacion = liq.id
inner join flow.regp_intervinientes inter on inter.liquidacion = dl.id
left join app.reg_acto act on act.id = dl.acto
left join app.cat_ente ent on ent.id = inter.ente
left join app.cat_ente bene on (bene.id = liq.beneficiario)
left join app.acl_user us on (us.id = liq.user_anular)
left join app.cat_ente ente_usuario on ente_usuario.id = us.ente
left join financiero.ren_nota_credito nc on nc.factura = liq.id
where (CASE WHEN $P{USUARIO}>0 THEN us.id=$P{USUARIO} ELSE us.id>0 END)
and liq.estado_liquidacion = 3
and liq.fecha_creacion between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without  time zone AND
to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
order by 1, 2, 3]]>
	</queryString>
