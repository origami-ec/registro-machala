	<queryString>
		<![CDATA[select substring(pro_claveacceso,25,15) as codigo_comprobante,
p.pro_codigo as num_proforma, p.pro_fechaing as fecha_ingreso, p.pro_cedulafact as ci_ruc, p.pro_nombrefact as solicitante, substring(p.pro_usuario,7) as usuario, p.pro_valreg as subtotal, p.pro_valmat as dcto, (p.pro_valreg-p.pro_valmat) as total
from historico.proforma p
where p.pro_fechaing between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
and p.pro_status = 1
order by p.pro_fechaing]]>
	</queryString>
