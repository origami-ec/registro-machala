	<queryString>
		<![CDATA[SELECT tra.nit,  tra.fcr, t.codigo as ttm, substr(tra.dtm, 1, 149) as dtm, tra.cca, tra.vcc, tra.ttb, substr(tra.drb, 1, 149) as drb, tra.acto, tra.movimiento,
tra.valor_debito, tra.valor_credito, tra.valor_efectivo, tra.valor_cheque, tra.valor_bienes,
tra.valor_servicios, tra.valor_tcredito, tra.total
FROM ctlg.uaf_tra tra
 INNER JOIN ctlg.uaf_tramite t ON t.id = tra.uaf_tramite
WHERE tra.periodo = $P{PERIODO} AND tra.seleccionado = true
ORDER BY CAST(tra.fcr AS BIGINT) ASC, tra.nit ASC]]>
	</queryString>
