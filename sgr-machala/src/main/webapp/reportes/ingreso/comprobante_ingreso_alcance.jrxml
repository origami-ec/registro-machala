	<subDataset name="dataset1" uuid="50671049-7b82-49e0-a977-2a7d8a4446cf">
		<parameter name="ID_TRAMITE" class="java.lang.Long"/>
		<queryString>
			<![CDATA[SELECT act.nombre as contrato, li.codigo_anterior, de.inscripcion,
de.cuantia, de.avaluo, de.valor_unitario, de.num_predio, de.recargo,
de.valor_total, de.recargo, de.cantidad, de.base_imponible, act.nrpm_transaccion,
de.descuento, de.reingreso
FROM financiero.ren_pago_rubro rpr
left join financiero.ren_factura rf on rf.pago = rpr.pago
left join app.reg_acto act on act.id = rpr.rubro
left join app.reg_libro li on li.id = act.libro
left join flow.regp_liquidacion_detalles de on de.liquidacion = rf.liquidacion
where rf.id = $P{ID_TRAMITE} and de.acto = rpr.rubro and de.liquidacion = rf.liquidacion]]>
		</queryString>
		<field name="contrato" class="java.lang.String"/>
		<field name="codigo_anterior" class="java.lang.Long"/>
		<field name="inscripcion" class="java.lang.Integer"/>
		<field name="cuantia" class="java.math.BigDecimal"/>
		<field name="avaluo" class="java.math.BigDecimal"/>
		<field name="valor_unitario" class="java.math.BigDecimal"/>
		<field name="num_predio" class="java.lang.Integer"/>
		<field name="recargo" class="java.math.BigDecimal"/>
		<field name="valor_total" class="java.math.BigDecimal"/>
		<field name="cantidad" class="java.lang.Integer"/>
		<field name="base_imponible" class="java.math.BigDecimal"/>
		<field name="nrpm_transaccion" class="java.lang.String"/>
		<field name="descuento" class="java.math.BigDecimal"/>
		<field name="reingreso" class="java.lang.Boolean"/>
	</subDataset>
	<parameter name="VALOR_STRING" class="java.lang.String"/>
	<parameter name="ID_LIQUIDACION" class="java.lang.Long"/>
	<parameter name="FOOTER" class="java.lang.String"/>
	<queryString>
		<![CDATA[select liq.num_tramite_rp, liq.codigo_comprobante as numero_comprobante, liq.repertorio,
ht.fecha_ingreso, ht.fecha_entrega, en.ci_ruc, liq.inscripcion, fact.observacion as inf_adicional_prof,
case en.es_persona when true then
coalesce(en.nombres,'')||' '||coalesce(en.apellidos,'')
else en.razon_social end AS solicitante,
en.telefono1, en.correo1, us.code as inscriptor,
liq.oficio_memo_referencia, usu.usuario as user_ingreso
from flow.regp_liquidacion liq
left join flow.historico_tramites ht on (ht.id = liq.tramite)
left join financiero.ren_factura fact on fact.liquidacion = liq.id
left join app.cat_ente en on (en.id = fact.solicitante)
left join financiero.ren_cajero ecaj on ecaj.id = fact.caja
left join app.acl_user usu on usu.id = ecaj.usuario
left join app.acl_user us on (us.id = liq.inscriptor)
--left join app.acl_user us1 on (us1.id = liq.user_ingreso)
where fact.id = $P{ID_LIQUIDACION}]]>
	</queryString>
