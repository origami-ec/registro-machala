	<queryString>
		<![CDATA[select re.id as ente, mo.id as movimiento,
re.nombre, ac.nombre as acto, pa.papel as papel,
mo.num_inscripcion, mo.fecha_inscripcion, mo.fecha_repertorio, mo.num_repertorio,
mo.folio_inicio, mo.folio_fin, li.nombre as libro, dom.nombre as domicilio,
ej.nombre as notaria
from app.reg_movimiento_cliente mc
left join app.reg_ente_interviniente re on (re.id = mc.ente_interv)
left join app.reg_papel pa on (pa.id = mc.papel)
left join app.reg_movimiento mo on (mo.id = mc.movimiento)
left join app.reg_acto ac on (ac.id = mo.acto)
left join app.reg_libro li on (li.id = mo.libro)
left join app.reg_ente_judiciales ej on ej.id = mo.ente_judicial
left join app.reg_domicilio dom on dom.id = mo.domicilio
where mo.estado = 'AC' and
(CASE WHEN $P{LIBRO}>0 THEN mo.libro = $P{LIBRO} ELSE mo.libro > 0 END)
and mo.fecha_inscripcion between $P{DESDE} and $P{HASTA}
order by re.nombre, mo.fecha_inscripcion, mo.num_inscripcion,
mo.num_repertorio]]>
	</queryString>
