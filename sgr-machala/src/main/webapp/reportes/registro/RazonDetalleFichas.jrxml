	<queryString>
		<![CDATA[select rf.clave_catastral as codigo_predial, pa.descripcion as parroquia,
rf.num_ficha,a.abreviatura from app.reg_movimiento_ficha rm
left join app.reg_ficha rf on (rm.ficha=rf.id)
left join app.reg_movimiento m on (m.id=rm.movimiento)
left join app.reg_acto a on  (m.acto=a.id)
left join app.cat_parroquia pa on pa.id = rf.parroquia
where m.id=$P{ID_MOVIMIENTO}
order by rf.num_ficha desc]]>
	</queryString>
