	<parameter name="USUARIO" class="java.lang.Long"/>
	<parameter name="DESDE" class="java.lang.String"/>
	<parameter name="HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT *
FROM (

    SELECT distinct
        det.tipo_pago,
        det.valor,
        liq.fecha_ingreso,
        liq.num_tramite_rp,

        CASE
            WHEN det.tipo_pago = 7 THEN det.d_fecha
            WHEN det.tipo_pago = 5 THEN det.tr_fecha
            WHEN det.tipo_pago = 3 THEN det.nc_fecha
            ELSE NULL
        END AS fecha_banco,

        COALESCE(btc.descripcion, db.descripcion, trb.descripcion, chb.descripcion) AS banco_tc,

        sol.ci_ruc,
        CASE
            WHEN sol.es_persona THEN
                UPPER(COALESCE(sol.apellidos,'') || ' ' || COALESCE(sol.nombres,''))
            ELSE
                UPPER(sol.razon_social)
        END AS solicitante,

        liq.estado_liquidacion,
        tp.nombre AS forma_pago,

        CASE
            WHEN det.tipo_pago = 7 THEN
                CASE WHEN det.d_fecha < date_trunc('day', liq.fecha_ingreso) THEN 1 ELSE 0 END
            WHEN det.tipo_pago = 5 THEN
                CASE WHEN det.tr_fecha < date_trunc('day', liq.fecha_ingreso) THEN 1 ELSE 0 END
	  WHEN det.tipo_pago = 3 THEN
                CASE WHEN det.nc_fecha < date_trunc('day', liq.fecha_ingreso) THEN 1 ELSE 0 END
            ELSE 0
        END AS es21205

    FROM financiero.ren_pago_detalle det
        INNER JOIN financiero.ren_tipo_pago tp  ON tp.id = det.tipo_pago
        INNER JOIN financiero.ren_pago pa       ON pa.id = det.pago
        INNER JOIN flow.regp_liquidacion liq    ON liq.id = pa.liquidacion
        left join flow.regp_liquidacion_detalles ld on ld.liquidacion = liq.id and ld.fecha_ingreso::date = pa.fecha_pago::date
	left join app.reg_acto act on act.id = ld.acto
        LEFT JOIN app.cat_ente sol              ON sol.id = liq.beneficiario

        LEFT JOIN financiero.ren_entidad_bancaria btc ON btc.id = det.tc_banco
        LEFT JOIN financiero.ren_entidad_bancaria db  ON db.id  = det.d_banco
        LEFT JOIN financiero.ren_entidad_bancaria trb ON trb.id = det.tr_banco
        LEFT JOIN financiero.ren_entidad_bancaria chb ON chb.id = det.ch_banco

    WHERE
        liq.estado_liquidacion IN (2,3)
        AND liq.estado_pago = 2
        AND liq.reingreso = FALSE and ( liq.inscripcion or act.solvencia = FALSE)
        AND det.tipo_pago IN (3,5,7)
        AND (CASE WHEN $P{USUARIO}>0 THEN liq.user_ingreso=$P{USUARIO} ELSE liq.user_ingreso>0 END)
        AND liq.fecha_ingreso BETWEEN
              TO_TIMESTAMP($P{DESDE}, 'DD/MM/YYYY HH24:MI')
          AND TO_TIMESTAMP($P{HASTA}, 'DD/MM/YYYY HH24:MI')

    UNION ALL

    SELECT distinct
        det.tipo_pago,
        det.valor,
        fa.fecha AS fecha_ingreso,
        fa.num_tramite AS num_tramite_rp,

        CASE
            WHEN det.tipo_pago = 7 THEN det.d_fecha
            WHEN det.tipo_pago = 5 THEN det.tr_fecha
            WHEN det.tipo_pago = 3 THEN det.nc_fecha
            ELSE NULL
        END AS fecha_banco,

        COALESCE(btc.descripcion, db.descripcion, trb.descripcion, chb.descripcion) AS banco_tc,

        sol.ci_ruc,
        CASE
            WHEN sol.es_persona THEN
                UPPER(COALESCE(sol.apellidos,'') || ' ' || COALESCE(sol.nombres,''))
            ELSE
                UPPER(sol.razon_social)
        END AS solicitante,

        CASE WHEN fa.estado THEN 2 ELSE 3 END AS estado_liquidacion,
        tp.nombre AS forma_pago,


        CASE
            WHEN det.tipo_pago = 7 THEN         -- DEPÓSITO
                CASE WHEN det.d_fecha < date_trunc('day', fa.fecha) THEN 1 ELSE 0 END
            WHEN det.tipo_pago = 5 THEN         -- TRANSFERENCIA
                CASE WHEN det.tr_fecha < date_trunc('day', fa.fecha) THEN 1 ELSE 0 END
	  WHEN det.tipo_pago = 3 THEN
                CASE WHEN det.nc_fecha < date_trunc('day', fa.fecha) THEN 1 ELSE 0 END
            ELSE 0
        END AS es21205

    FROM financiero.ren_factura fa
        INNER JOIN financiero.ren_pago pa              ON fa.pago = pa.id
        INNER JOIN financiero.ren_pago_detalle det     ON pa.id = det.pago
        INNER JOIN financiero.ren_tipo_pago tp         ON tp.id = det.tipo_pago
        LEFT JOIN app.cat_ente sol                     ON sol.id = fa.solicitante
        LEFT JOIN financiero.ren_cajero ca             ON ca.id = fa.caja
        LEFT JOIN app.acl_user us                      ON us.id = ca.usuario

        LEFT JOIN financiero.ren_entidad_bancaria btc ON btc.id = det.tc_banco
        LEFT JOIN financiero.ren_entidad_bancaria db  ON db.id  = det.d_banco
        LEFT JOIN financiero.ren_entidad_bancaria trb ON trb.id = det.tr_banco
        LEFT JOIN financiero.ren_entidad_bancaria chb ON chb.id = det.ch_banco
        LEFT JOIN flow.regp_liquidacion liq ON liq.id = pa.liquidacion
	left join flow.regp_liquidacion_detalles ld on ld.liquidacion = liq.id and ld.fecha_ingreso::date = pa.fecha_pago::date
	left join app.reg_acto ac on ac.id = ld.acto
    WHERE
        det.tipo_pago IN (3,5,7)
        AND fa.estado = TRUE and ac.solvencia = false
        AND (CASE WHEN $P{USUARIO}>0 THEN us.id=$P{USUARIO} ELSE us.id>0 END)
        AND fa.fecha BETWEEN
              TO_TIMESTAMP($P{DESDE}, 'DD/MM/YYYY HH24:MI')
          AND TO_TIMESTAMP($P{HASTA}, 'DD/MM/YYYY HH24:MI')
) AS pagos


WHERE es21205 = 1

ORDER BY fecha_ingreso, num_tramite_rp;]]>
	</queryString>
