	<queryString>
		<![CDATA[SELECT row_number() over(ORDER BY c.id) numero, c.num_certificado AS numero_secuencia, ra.nombre, c.num_tramite, u.usuario, CAST(ht.fecha_ingreso AS DATE) AS fecha_ingreso, CAST(ht.fecha_entrega AS DATE) AS fecha_entrega,ht.fecha_ingreso as fecha_i, ht.fecha_entrega as fech_en,
replace(ra.nombre, 'CERTIFICADO ', '') as tipo_certificado,CAST(c.fecha_emision AS DATE) AS fecha_emision , c.fecha_emision as fecha_em,
TRIM(COALESCE(ue.nombres, '') || ' ' || COALESCE(ue.apellidos, '')) nombre_funcionario
FROM app.reg_certificado c
INNER JOIN flow.regp_tareas_tramite tt ON tt.id = c.tarea_tramite
INNER JOIN flow.regp_liquidacion_detalles ld ON ld.id = tt.detalle
INNER JOIN app.reg_acto ra ON ra.id = ld.acto
INNER JOIN flow.regp_liquidacion l ON l.id = ld.liquidacion
LEFT JOIN app.cat_ente sl ON sl.id = l.solicitante
INNER JOIN flow.historico_tramites ht ON ht.id = tt.tramite
INNER JOIN app.acl_user u ON u.id = c.user_creador
INNER JOIN app.cat_ente ue ON ue.id = u.ente
WHERE tt.realizado = true AND tt.estado = true
AND (c.fecha_emision between to_timestamp($P{FECHA}, 'DD/MM/YYYY')::timestamp without  time zone AND
to_timestamp($P{HASTA}, 'DD/MM/YYYY')::timestamp without time zone)
AND CASE WHEN $P{USUARIO} IS NULL THEN true ELSE u.id = $P{USUARIO} END
ORDER BY c.num_certificado ASC]]>
	</queryString>
