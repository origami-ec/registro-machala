	<queryString>
		<![CDATA[select sol.ci_ruc, sol.apellidos || sol.nombres as nombres_completos, o.observacion1 as observacion, o.numero, us.usuario
 from flow.Regp_Observaciones_Ingreso o
left join app.acl_user us on us.id = o.user_ingreso
left join app.cat_ente sol on sol.id = o.ente
where
(CASE WHEN $P{USUARIO} > 0 THEN us.id = $P{USUARIO} ELSE us.id > 0 END) and
o.fecha_ingreso between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
order by o.fecha_ingreso]]>
	</queryString>
