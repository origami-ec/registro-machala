	<parameter name="USUARIO" class="java.lang.Long"/>
	<parameter name="DESDE" class="java.lang.String"/>
	<parameter name="HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
    det.tipo_pago,
    det.valor,
    l.fecha_ingreso,
    l.num_tramite_rp,

    CASE
        WHEN det.tipo_pago = 7 THEN det.d_fecha       -- Depósito
        WHEN det.tipo_pago = 5 THEN det.tr_fecha      -- Transferencia
        WHEN det.tipo_pago = 3 THEN det.nc_fecha      -- Nota Crédito
        ELSE NULL
    END AS fecha_banco,

    COALESCE(bt.descripcion, bd.descripcion, 'BANCO NO REGISTRADO') AS banco_tc,

    bene.ci_ruc,
    CASE bene.es_persona
        WHEN true THEN UPPER(coalesce(bene.apellidos,'')||' '||coalesce(bene.nombres,''))
        ELSE UPPER(bene.razon_social)
    END AS solicitante,

    l.estado_liquidacion,

    CASE
        WHEN det.tipo_pago = 7 THEN 'DEPÓSITO'
        WHEN det.tipo_pago = 5 THEN 'TRANSFERENCIA'
        WHEN det.tipo_pago = 3 THEN 'NOTA DE CRÉDITO'
        ELSE 'OTRO'
    END AS forma_pago,

    1 AS es21205

FROM flow.regp_liquidacion l
LEFT JOIN flow.regp_liquidacion_detalles ld ON ld.liquidacion = l.id AND ld.fecha_ingreso::date = l.fecha_creacion::date
LEFT JOIN app.reg_acto act ON act.id = ld.acto
LEFT JOIN app.acl_user us ON (us.id = l.user_ingreso)
LEFT JOIN app.cat_ente ente_usuario ON ente_usuario.id = us.ente
LEFT JOIN app.cat_ente bene ON bene.id = l.beneficiario
LEFT JOIN financiero.ren_pago pa ON pa.liquidacion = l.id AND l.fecha_ingreso::date = pa.fecha_pago::date
LEFT JOIN financiero.ren_pago_detalle det ON det.pago = pa.id


LEFT JOIN financiero.ren_entidad_bancaria bd ON bd.id = det.d_banco
LEFT JOIN financiero.ren_entidad_bancaria bt ON bt.id = det.tr_banco

WHERE l.estado_liquidacion = 2
  AND l.estado_pago = 2
  AND (CASE WHEN $P{USUARIO} > 0 THEN l.user_ingreso = $P{USUARIO} ELSE l.user_ingreso > 0 END)


  AND act.solvencia


  AND det.tipo_pago IN (3, 5, 7)


  AND l.fecha_ingreso BETWEEN
      CAST(to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI') AS timestamp without time zone)
      AND CAST(to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI') AS timestamp without time zone)

  AND (
       (det.tipo_pago = 7 AND det.d_fecha < date_trunc('day', l.fecha_ingreso)) OR
       (det.tipo_pago = 5 AND det.tr_fecha < date_trunc('day', l.fecha_ingreso)) OR
       (det.tipo_pago = 3 AND det.nc_fecha < date_trunc('day', l.fecha_ingreso))
  )

GROUP BY
    det.tipo_pago,
    det.valor,
    det.d_fecha,
    det.tr_fecha,
    det.nc_fecha,
    bt.descripcion,
    bd.descripcion,
    l.fecha_ingreso,
    l.num_tramite_rp,
    bene.ci_ruc,
    bene.es_persona,
    bene.apellidos,
    bene.nombres,
    bene.razon_social,
    l.estado_liquidacion
ORDER BY l.fecha_ingreso, l.num_tramite_rp;]]>
