	<subDataset name="REFERENCIAS" uuid="4040ef36-dfb8-4392-9353-a72e6084d9fa">
		<parameter name="MOVIMIENTO" class="java.lang.Long">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="INDEX" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT row_number() over(ORDER BY m.fecha_inscripcion) numero, m.fecha_inscripcion,
a.nombre contrato,m.num_repertorio, m.fecha_repertorio, m.num_inscripcion, m.id movimiento
FROM app.reg_movimiento_referencia mvr INNER JOIN app.reg_movimiento m ON m.id = mvr.movimiento_referencia
LEFT JOIN app.reg_libro l ON l.id = m.libro
LEFT JOIN app.reg_acto a ON a.id = m.acto
WHERE mvr.movimiento = $P{MOVIMIENTO}
ORDER BY m.fecha_inscripcion]]>
		</queryString>
		<field name="numero" class="java.lang.Long"/>
		<field name="fecha_inscripcion" class="java.sql.Timestamp"/>
		<field name="contrato" class="java.lang.String"/>
		<field name="num_repertorio" class="java.lang.Integer"/>
		<field name="fecha_repertorio" class="java.sql.Timestamp"/>
		<field name="num_inscripcion" class="java.lang.Integer"/>
		<field name="movimiento" class="java.lang.Long"/>
	</subDataset>
	<parameter name="FECHA" class="java.util.Date">
		<defaultValueExpression><![CDATA[new Date()]]></defaultValueExpression>
	</parameter>
	<parameter name="USUARIO" class="java.lang.Long">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="IMG_URL" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA["D:\\PROYECTOS\\PROYECTOS\\registropropiedad\\src\\main\\webapp\\resources\\image\\logo.jpg"]]></defaultValueExpression>
	</parameter>
	<parameter name="LIBRO" class="java.lang.Long"/>
	<queryString>
		<![CDATA[SELECT row_number() over(ORDER BY r.fecha_inscripcion) numero, ra.nombre contrato,
r.num_repertorio, r.fecha_repertorio, r.num_inscripcion, r.fecha_inscripcion, UPPER(u.usuario) usuario,
TRIM(COALESCE(ue.nombres, '') || ' ' || COALESCE(ue.apellidos, '')) nombre_funcionario, r.id movimiento
, a.nombre reff_acto, m.fecha_inscripcion reff_fecha_insc,
case when ra.id='40' then r.observacion else ' ' end as obs_plano
FROM app.reg_movimiento m INNER JOIN app.reg_movimiento_referencia mr ON mr.movimiento = m.id
INNER JOIN app.reg_movimiento r ON r.id = mr.movimiento_referencia
LEFT JOIN app.reg_libro l ON l.id = m.libro
LEFT JOIN app.reg_acto a ON a.id = m.acto
INNER JOIN app.acl_user u ON u.id = m.user_creador
INNER JOIN app.cat_ente ue ON ue.id = u.ente
LEFT JOIN app.reg_acto ra ON ra.id = r.acto
WHERE m.fecha_inscripcion::DATE = CAST($P{FECHA} AS DATE)
AND r.fecha_inscripcion::date <> CAST($P{FECHA} AS DATE)
AND CASE WHEN $P{USUARIO} IS NULL THEN true ELSE u.id = $P{USUARIO} END
AND CASE WHEN $P{LIBRO} IS NULL THEN true ELSE a.libro = $P{LIBRO} END
ORDER BY r.fecha_inscripcion;]]>
	</queryString>
