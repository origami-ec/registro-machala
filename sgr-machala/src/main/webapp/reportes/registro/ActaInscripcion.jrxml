	<queryString>
		<![CDATA[select to_char(li.fecha_ingreso,'yyyy') as anio, rm.avaluo_municipal, rm.cuantia,
rm.num_tomo, rm.num_repertorio, rm.num_inscripcion, acto.nombre as acto,
libro.nombre as libro, rm.fecha_inscripcion, rm.fecha_oto, rm.fecha_repertorio,
j.nombre as entejudicial, rm.escrit_juic_prov_resolucion as escritura, rm.fecha_resolucion,  canton.nombre as canton, rm.observacion, rm.folio_inicio, rm.folio_fin, usuario.usuario as user_inscrip,
coalesce(ente.nombres, '') ||' '|| coalesce(ente.apellidos, '') as usuario,
reg.nombre_completo, reg.titulo_completo, ht.num_tramite, li.codigo_comprobante,
libro.propiedad, rm.cod_verificacion, li.codigo_comprobante comprobante,
(select valor_string from conf.valores where code = 'URL_VALIDACION_DOCUMENTOS')
|| rm.cod_verificacion as qrcode,
(select id from app.reg_movimiento_representante where movimiento = $P{P_MOVIMIENTO} limit 1)
as representante, (select id from app.reg_movimiento_socios where movimiento = $P{P_MOVIMIENTO} limit 1) as socios, (select id from app.reg_movimiento_ficha where movimiento = $P{P_MOVIMIENTO} limit 1) as fichas, (select id from app.reg_movimiento_capital where movimiento = $P{P_MOVIMIENTO} limit 1) as capital,
(select id from app.reg_movimiento_referencia where movimiento = $P{P_MOVIMIENTO} limit 1) as referencia,
usuario.firma_nombre as firma_nombre1, usuario.firma_cargo as firma_cargo1,
je.firma_nombre as firma_nombre2, je.firma_cargo as firma_cargo2, je.usuario as user_jefe,
indeterminada, descripcion_bien
from app.reg_movimiento rm
left join app.reg_acto acto on(rm.acto=acto.id)
left join app.reg_libro libro on(rm.libro=libro.id)
left join app.reg_ente_judiciales j on(rm.ente_judicial=j.id)
left join app.reg_domicilio canton on(rm.domicilio=canton.id)
left join app.acl_user usuario on(rm.user_creador=usuario.id)
left join app.cat_ente ente on (ente.id = usuario.ente)
left join app.reg_registrador reg on(rm.registrador=reg.id)
left join flow.regp_tareas_tramite tt on (tt.id = rm.num_tramite)
left join flow.regp_liquidacion_detalles de on de.id = tt.detalle
left join flow.historico_tramites ht on (ht.id = tt.tramite)
left join flow.regp_liquidacion li on (li.tramite = ht.id)
left join app.acl_user re on(li.user_creacion = re.id)
left join app.cat_ente rev on (rev.id = re.ente)
--left join app.acl_user je on rm.jefe_inscripciones = je.id
left join app.acl_user je on je.usuario = $P{JEFE_INSCRIPCION}
where rm.id = $P{P_MOVIMIENTO}]]>
	</queryString>
