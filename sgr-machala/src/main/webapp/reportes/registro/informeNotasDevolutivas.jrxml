	<queryString>
		<![CDATA[SELECT
ht.num_tramite, nod.num_nota_devolutiva,
flow.detalle_nota_devolutiva(nod.detalle) detalle_nota_devolutiva, nod.realizado,
to_char(nod.fecha_ingreso, 'DD-MON') ingreso,
to_char(nod.fecha_retiro, 'DD-MON') retiro,
to_char(nod.fecha_reingreso, 'DD-MON') reingreso,
to_char(nod.fecha_entrega_tramite, 'DD-MON') entrega_tramite,
flow.observacion_nota_devolutiva(nod.observacion, nod.quien_retira, nod.quien_reingresa) obs
FROM flow.regp_nota_devolutiva nod
inner join flow.historico_tramites ht on nod.tramite = ht.id
where nod.fecha_ingreso::date between $P{DESDE} and $P{HASTA}
AND CASE WHEN $P{USUARIO} IS NULL THEN true ELSE nod.elaborado = $P{USUARIO} END
order by nod.id asc]]>
	</queryString>
