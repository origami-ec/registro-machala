	<queryString>
		<![CDATA[select li.id, li.num_tramite_rp, li.fecha_creacion, li.sub_total, li.descuento_valor,
li.total_pagar, li.codigo_comprobante, li.numero_comprobante, us.usuario,
li.inscripcion, li.reingreso, sol.ci_ruc,
case sol.es_persona when true then
coalesce(sol.apellidos,'')||' '||coalesce(sol.nombres,'') else sol.razon_social end as solicitante
from flow.regp_liquidacion li
left join app.acl_user us on us.id = li.user_creacion
left join app.cat_ente sol on sol.id = li.solicitante
where (CASE WHEN $P{USUARIO} > 0 THEN us.id = $P{USUARIO} ELSE us.id > 0 END)
and li.fecha_creacion between to_timestamp($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone AND to_timestamp($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp without time zone
order by 2]]>
	</queryString>
