	<queryString>
		<![CDATA[select row_number() over(ORDER BY rt.fin_time) numero, rt.num_tramite, rt.fecha_ingreso, rt.nombre_propietario, rt.name_ as tarea, fin_time, substring(rt.descripcion, 12) as proceso
from flow.reporte_tareas rt
WHERE (rt.fin_time between to_timestamp($P{FECHA}, 'DD/MM/YYYY')::timestamp without time zone AND
to_timestamp($P{HASTA}, 'DD/MM/YYYY')::timestamp without time zone)
AND CASE WHEN $P{USUARIO} IS NULL THEN true ELSE rt.assignee = $P{NAME_USER} END
ORDER BY rt.fin_time]]>
	</queryString>
