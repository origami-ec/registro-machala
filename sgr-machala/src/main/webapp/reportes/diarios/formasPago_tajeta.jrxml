	<parameter name="USUARIO" class="java.lang.Long"/>
	<parameter name="DESDE" class="java.lang.String"/>
	<parameter name="HASTA" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT *
FROM (
    SELECT
        det.tipo_pago,
        det.valor,
        det.tc_num_tarjeta,
        det.tc_fecha_caducidad,
        det.tc_autorizacion,
        det.tc_baucher,
        COALESCE(_btc.descripcion, _db.descripcion, _trb.descripcion, _chb.descripcion) AS banco_tc,
        det.tc_titular,
        liq.num_tramite_rp,
        liq.numero_comprobante,
        CASE
            WHEN sol.es_persona THEN COALESCE(sol.apellidos,'') || ' ' || COALESCE(sol.nombres,'')
            ELSE sol.razon_social
        END AS solicitante,
        sol.ci_ruc,
        liq.fecha_ingreso,
        us.usuario,
        liq.codigo_comprobante,
        'TARJETAS' AS forma_pago,

        CASE
            WHEN tp.id = 2 THEN 'TARJETA DE CRÉDITO'
            ELSE tp.nombre
        END AS tipo_tarjeta,

        estado_liquidacion
    FROM financiero.ren_pago_detalle det
        LEFT JOIN financiero.ren_tipo_pago tp ON tp.id = det.tipo_pago
        LEFT JOIN financiero.ren_pago pa ON pa.id = det.pago
        LEFT JOIN flow.regp_liquidacion liq ON liq.id = pa.liquidacion
        LEFT JOIN app.cat_ente sol ON sol.id = liq.beneficiario
        LEFT JOIN app.acl_user us ON us.id = liq.user_ingreso
        LEFT JOIN financiero.ren_entidad_bancaria _btc ON _btc.id = det.tc_banco
        LEFT JOIN financiero.ren_entidad_bancaria _db ON _db.id = det.d_banco
        LEFT JOIN financiero.ren_entidad_bancaria _trb ON _trb.id = det.tr_banco
        LEFT JOIN financiero.ren_entidad_bancaria _chb ON _chb.id = det.ch_banco
    WHERE
        liq.estado_liquidacion IN (2,3)
        AND liq.estado_pago = 2
        AND liq.reingreso = FALSE
        AND tp.id = 2
        AND (CASE WHEN $P{USUARIO}>0 THEN liq.user_ingreso=$P{USUARIO} ELSE liq.user_ingreso>0 END)
        AND pa.id NOT IN (SELECT pago FROM financiero.ren_factura fa WHERE fa.estado = TRUE)
        AND liq.fecha_ingreso BETWEEN TO_TIMESTAMP($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp
                                  AND TO_TIMESTAMP($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp

    UNION ALL

    SELECT
        detalle.tipo_pago,
        detalle.valor,
        detalle.tc_num_tarjeta,
        detalle.tc_fecha_caducidad,
        detalle.tc_autorizacion,
        detalle.tc_baucher,
        btc.descripcion AS banco_tc,
        detalle.tc_titular,
        fa.num_tramite AS num_tramite_rp,
        fa.numero_comprobante,
        CASE
            WHEN sol.es_persona THEN COALESCE(sol.apellidos,'') || ' ' || COALESCE(sol.nombres,'')
            ELSE sol.razon_social
        END AS solicitante,
        sol.ci_ruc,
        fa.fecha AS fecha_ingreso,
        us.usuario,
        fa.codigo_comprobante,
        'TARJETAS' AS forma_pago,

        CASE
            WHEN tp.id = 2 THEN 'TARJETA DE CRÉDITO'
            ELSE tp.nombre
        END AS tipo_tarjeta,

        CASE WHEN fa.estado THEN 2 ELSE 3 END AS estado_liquidacion
    FROM financiero.ren_factura fa
        LEFT JOIN financiero.ren_pago pa ON fa.pago = pa.id
        LEFT JOIN financiero.ren_pago_detalle detalle ON pa.id = detalle.pago
        LEFT JOIN financiero.ren_entidad_bancaria btc ON btc.id = detalle.tc_banco
        INNER JOIN financiero.ren_tipo_pago tp ON tp.id = detalle.tipo_pago
        LEFT JOIN financiero.ren_cajero ca ON ca.id = fa.caja
        LEFT JOIN app.acl_user us ON us.id = ca.usuario
        LEFT JOIN app.cat_ente sol ON sol.id = fa.solicitante
    WHERE
        (CASE WHEN $P{USUARIO}>0 THEN us.id=$P{USUARIO} ELSE us.id>0 END)
        AND fa.fecha BETWEEN TO_TIMESTAMP($P{DESDE}, 'DD/MM/YYYY HH24:MI')::timestamp
                        AND TO_TIMESTAMP($P{HASTA}, 'DD/MM/YYYY HH24:MI')::timestamp
        AND fa.estado = TRUE
        AND tp.id = 2
) dum
ORDER BY forma_pago, num_tramite_rp;]]>
